<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.habibcse009.studentinfodatabase">

    <application
        android:allowBackup="true"
        android:icon="@drawable/userlogo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/userlogo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".DetailsActivity"></activity>
        <activity android:name=".ListInfoActivity" />
        <activity android:name=".AddInfoActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>